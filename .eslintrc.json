{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["@typescript-eslint", "tailwindcss"],
  "rules": {
    "tailwindcss/no-custom-classname": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
